    },
    "homepage": "https://github.com/horoshopodumaj/webfull#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "config": "^3.3.8",
        "express": "^4.18.2",
        "express-validator": "^6.14.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.7.2"
    },
    "devDependencies": {
        "concurrently": "^7.5.0",
        "nodemon": "^2.0.20"
    }
